put\"\n             placeholder=\"Last Name\"\n             type=\"text\"\n             value={lastName}\n             onChange={(e) => setLastName(e.target.value)}\n         />\n         <br /> \n         <br />\n         <br />\n         <br />\n         <input \n            className=\"signup-input\"\n            placeholder=\"Username\"\n             type=\"text\"\n             value={username}\n             onChange={(e) => setUsername(e.target.value)}\n         />\n         <br />\n         <br /> \n         <br /> \n         <br />  \n         <input \n            className=\"signup-input\"\n            placeholder=\"Password\"\n             type=\"password\"\n             value={password}\n             onChange={(e) => setPassword(e.target.value)}\n         />\n         <br /> \n         <br />\n         <br /> \n         <br /> \n         <input \n            className=\"signup-input\"\n            placeholder=\"Monthly income after taxes\"\n             type=\"text\"\n             value={monthlyIncome}\n             onChange={(e) => setMonthlyIncome(e.target.value)}\n         />\n         <br /> \n         <br /> \n         <br /> \n         <input className=\"signup-button\" type=\"submit\" value=\"Submit\" />\n        </form>\n    </div>\n       \n    )\n}\n\n\nexport default SignUp","/Users/DavidMedina/Development/Flatiron/code/Phase5/BudEx-frontend/src/ExpenseForm.js",[],{"ruleId":"62","severity":1,"message":"63","line":11,"column":12,"nodeType":"64","messageId":"65","endLine":11,"endColumn":18},{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":29,"column":36,"nodeType":"72","messageId":"73","endLine":29,"endColumn":44},{"ruleId":"74","severity":1,"message":"75","line":56,"column":123,"nodeType":"76","messageId":"77","endLine":56,"endColumn":125},{"ruleId":"74","severity":1,"message":"75","line":57,"column":121,"nodeType":"76","messageId":"77","endLine":57,"endColumn":123},{"ruleId":"7